<p id="notice"><%= notice %></p>

<h1>Telphones</h1>

<table>
  <thead>
    <tr>
      <th>Num1</th>
      <th>Num2</th>
      <th>Num3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telphones.each do |telphone| %>
      <tr>
        <td><%= telphone.num1 %></td>
        <td><%= telphone.num2 %></td>
        <td><%= telphone.num3 %></td>
        <td><%= link_to 'Show', telphone %></td>
        <td><%= link_to 'Edit', edit_telphone_path(telphone) %></td>
        <td><%= link_to 'Destroy', telphone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Telphone', new_telphone_path %>
